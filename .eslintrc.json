{
  "extends": ["defaults"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint", 
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "quotes":"off",
    "react/destructuring-assignment":"off",
    "react/jsx-one-expression-per-line":"off",
    "indent":"off",
    "no-var":"off"
  }
}
